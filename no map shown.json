{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 500,
  "height": 300,
  "data": {
    "url": "https://raw.githubusercontent.com/vega/vega-datasets/master/data/us-10m.json",
    "format": {"type": "topojson", "feature": "counties"}
  },
  "transform": [
    {
      "lookup": "id",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/CD30510023/3179repo/main/aqi_yearly_1980_to_2021.csv"
        },
        "key": "County",
        "fields": ["Median AQI"]
      }
    }
  ],
  "projection": {"type": "albersUsa"},
  "mark": "geoshape",
  "encoding": {
    "color": {
      "field": "Median AQI",
      "type": "quantitative",
      "scale": {
        "type": "threshold",
        "domain": [32, 39, 46],
        "range": ["#fdbe85", "#fd8d3c", "#e6550d", "#a63603"]
      }
    },
    "tooltip": [
      {"field": "properties.NAME", "type": "nominal", "title": "Country"},
      {"field": "Median.AQI", "type": "quantitative", "title": "AQI"}
    ]
  },
  "config": {}
}